@prefix ns1: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://schema.org/CSVWTableSchema> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:description """
    An object property that provides a single schema description, used as the 
    default for all the tables in the group.
    """ ;
    ns1:ignoredProperties ( rdf:type rdfs:label rdfs:comment ) ;
    ns1:message """
    TableSchema description for a CSVW Table. 1 csvw:columns is required.
    """ ;
    ns1:name "csvw:TableSchema" ;
    ns1:property [ ns1:description """
        csvw:column with string values interpreted as identifier with array 
        values interpreted as rdf:List.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        At least one CSVW Column is required.
        """ ;
            ns1:minCount 1 ;
            ns1:name "csvw:columns" ;
            ns1:node <http://datashapes.org/dash#ListShape> ;
            ns1:path <http://www.w3.org/ns/csvw#columns> ;
            ns1:property [ ns1:closed true ;
                    ns1:minCount 1 ;
                    ns1:path ( [ ns1:zeroOrMorePath rdf:rest ] rdf:first ) ;
                    ns1:property [ ns1:datatype xsd:string ;
                            ns1:description """
                An atomic property that gives a single canonical name for the 
                column. The value of this property becomes the name annotation 
                for the described column.
                May only be provided once.
                """ ;
                            ns1:maxCount 1 ;
                            ns1:message """
                csvw:name must be a string and is required.
                """ ;
                            ns1:minCount 1 ;
                            ns1:minLength 1 ;
                            ns1:name "csvw:name" ;
                            ns1:nodeKind ns1:Literal ;
                            ns1:path <http://www.w3.org/ns/csvw#name> ],
                        [ ns1:datatype xsd:string ;
                            ns1:description """
                An atomic property giving the string or strings used for null 
                values within the data. If the string value of the cell is 
                equal to any one of these values, the cell value is `null`.
                """ ;
                            ns1:maxCount 1 ;
                            ns1:message """
                csvw:null must be a string and is optional.
                May only be provided once.
                """ ;
                            ns1:minCount 0 ;
                            ns1:name "csvw:null" ;
                            ns1:nodeKind ns1:Literal ;
                            ns1:path <http://www.w3.org/ns/csvw#null> ] ] ] ;
    ns1:targetClass <http://www.w3.org/ns/csvw#TableSchema> .

<http://schema.org/D2RQDatabaseShape> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:description """
    An RML Logical Source: D2RQ Database describes how a relational database 
    needs to be accessed.
    """ ;
    ns1:ignoredProperties ( rdf:type rdfs:label rdfs:comment ) ;
    ns1:message """
    An RML Logical Source description for a D2RQ Database requires exactly
    one d2rq:jdbcDSN, one d2rq:jdbcDriver. d2rq:username and d2rq:password are
    optional.
    """ ;
    ns1:name "RML Logical Source description: D2RQ Database" ;
    ns1:or ( [ ns1:and ( [ ns1:minCount 1 ;
                            ns1:path <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#username> ] [ ns1:minCount 1 ;
                            ns1:path <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#password> ] ) ] [ ns1:and ( [ ns1:minCount 0 ;
                            ns1:path <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#username> ] [ ns1:maxCount 0 ;
                            ns1:path <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#password> ] ) ] ) ;
    ns1:property [ ns1:datatype xsd:string ;
            ns1:description """
        The JDBC database URL.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        The JDBC database URL must be a string of the form: 
        'jdbc:{subprotocol}:{subname}://{hostname}:{port}/{dbname}'
        and may only be provided once.
        """ ;
            ns1:minCount 1 ;
            ns1:name "d2rq:jdbcDSN" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#jdbcDSN> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        The JDBC driver class name for the database. Used together with 
        d2rq:jdbcDSN.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        The JDBC driver for the database must be specified.
        """ ;
            ns1:minCount 1 ;
            ns1:name "d2rq:jdbcDriver" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#jdbcDriver> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        A username if required by the database.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        The username for the database must be a string. 
        """ ;
            ns1:minCount 0 ;
            ns1:name "d2rq:username" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#username> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        A password if required by the database.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        The password for the database must be a string. 
        """ ;
            ns1:minCount 0 ;
            ns1:name "d2rq:password" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#password> ] ;
    ns1:targetClass <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#Database> .

<http://schema.org/RRLogicalTable> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:description """
    Represents a logical table.
    """ ;
    ns1:ignoredProperties ( rdf:type rdfs:label rdfs:comment ) ;
    ns1:message """
    rr:LogicalTable must provide either one rr:tableName or one rr:sqlQuery.
    rr:sqlVersion is optional. 
    """ ;
    ns1:name "rr:LogicalTable" ;
    ns1:property [ ns1:description """
        Exactly one rr:tableName or one rr:sqlQuery is required.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Exactly one rr:tableName or one rr:sqlQuery is required.
        """ ;
            ns1:minCount 1 ;
            ns1:name "rr:tableName/rr:sqlQuery" ;
            ns1:path [ ns1:alternativePath ( <http://www.w3.org/ns/r2rml#tableName> <http://www.w3.org/ns/r2rml#sqlQuery> ) ] ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        Schema-qualified name of a table or view.
        """ ;
            ns1:message """
        rr:tableName must be a string and may only be provided once.
        """ ;
            ns1:name "rr:tableName" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/r2rml#tableName> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        A valid SQL query.
        """ ;
            ns1:message """
        rr:sqlQuery must be a string and may only be provided once.
        """ ;
            ns1:name "rr:sqlQuery" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/r2rml#sqlQuery> ],
        [ ns1:description """
        An identifier for a SQL version.
        """ ;
            ns1:in ( <http://www.w3.org/ns/r2rml#SQL2008> ) ;
            ns1:message """
        rr:sqlVersion may only be provided once and must its value must be 
        rr:SQL2008.
        """ ;
            ns1:name "rr:sqlVersion" ;
            ns1:nodeKind ns1:IRI ;
            ns1:path <http://www.w3.org/ns/r2rml#sqlVersion> ] .

<http://schema.org/RRTriplesMapShape> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:description """
    Represents a triples map.
    """ ;
    ns1:ignoredProperties ( rdf:type rdfs:label rdfs:comment ) ;
    ns1:message """
    rr:TriplesMap requires exactly one rr:subject or one rr:subjectMap and zero
    or more rr:predicateObjectMaps.
    """,
        "rr:TriplesMap" ;
    ns1:property [ ns1:description """
        An IRI reference for use as subject for all the RDF triples generated 
        from a logical table row or iterator.
        """ ;
            ns1:message """
        rr:subject must be an IRI or blank node.
        """ ;
            ns1:name "rr:subject" ;
            ns1:node <http://schema.org/RRsubjectShape> ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <http://www.w3.org/ns/r2rml#subject> ],
        [ ns1:description """
        A PredicateObjectMap element to generate (predicate, object) pair from 
        a logical table row.
        """ ;
            ns1:message """
        rr:PredicateObjectMap must be an IRI or blank node.
        """ ;
            ns1:minCount 0 ;
            ns1:name "rr:predicateObjectMap" ;
            ns1:node <http://schema.org/RRPredicateObjectMapShape> ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <http://www.w3.org/ns/r2rml#predicateObjectMap> ],
        [ ns1:description """
        Either one rr:logicalTable or one rml:logicalSource is required, not 
        both.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Either one rr:logicalTable or one rml:logicalSource is required, not 
        both.
        """ ;
            ns1:minCount 1 ;
            ns1:name "rr:logicalTable/rml:logicalSource" ;
            ns1:path [ ns1:alternativePath ( <http://www.w3.org/ns/r2rml#logicalTable> <http://semweb.mmlab.be/ns/rml#logicalSource> ) ] ],
        [ ns1:description """
        Definition of logical table to be mapped.
        """ ;
            ns1:message """
        Exactly one rr:logicalTable is required to access a relational 
        database.
        """ ;
            ns1:name "rr:logicalTable" ;
            ns1:node <http://schema.org/RRLogicalTableShape> ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <http://www.w3.org/ns/r2rml#logicalTable> ],
        [ ns1:description """
        A logical source is any source that is mapped to RDF triples. A logical 
        source is a Base Source, rml:BaseSource.
        """ ;
            ns1:message """
        Exactly one rml:logicalSource is required to access the data source.
        """ ;
            ns1:name "rml:logicalSource" ;
            ns1:node <http://schema.org/RMLLogicalSourceShape> ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <http://semweb.mmlab.be/ns/rml#logicalSource> ],
        [ ns1:description """
        Either one rr:subject or one rr:SubjectMap is required, not both.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Either one rr:subject or one rr:SubjectMap is required, not both.
        """ ;
            ns1:minCount 1 ;
            ns1:name "rr:subjectMap/rr:subject" ;
            ns1:path [ ns1:alternativePath ( <http://www.w3.org/ns/r2rml#subjectMap> <http://www.w3.org/ns/r2rml#subject> ) ] ],
        [ ns1:description """
        A SubjectMap element to generate a subject from a logical table row or
        iterator.
        """ ;
            ns1:message """
        rr:SubjectMap must be an IRI or blank node.
        """ ;
            ns1:name "rr:subjectMap" ;
            ns1:node <http://schema.org/RRSubjectMapShape> ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <http://www.w3.org/ns/r2rml#subjectMap> ] ;
    ns1:targetClass <http://www.w3.org/ns/r2rml#TriplesMap> .

<http://schema.org/CSVFileShape> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:property [ ns1:description """
        The source (rml:source) locates the input data source. It is a URI
        that represents the data source where the data source is.
        
        Note: A literal as rml:source is still supported for compatibility with 
        previous versions of RML.
        """ ;
            ns1:message """
        rml:source must be provided to locate the data source. rml:source must 
        be provided exactly once as an URI or Literal for backwards 
        compatiblity with previous versions of RML.

        Locating CSV files is possible using a DCAT dataset description, 
        a CSVW Table description or a file path to a local file.
        """ ;
            ns1:name "rml:source" ;
            ns1:path <http://semweb.mmlab.be/ns/rml#source> ;
            ns1:xone ( [ ns1:datatype xsd:string ;
                        ns1:nodeKind ns1:Literal ] [ ns1:class <http://www.w3.org/ns/csvw#Table> ;
                        ns1:node <http://schema.org/CSVWTableShape> ;
                        ns1:nodeKind ns1:BlankNodeOrIRI ] [ ns1:class <http://www.w3.org/ns/dcat#Dataset> ;
                        ns1:node <http://schema.org/DCATDatasetShape> ;
                        ns1:nodeKind ns1:BlankNodeOrIRI ] ) ],
        [ ns1:description """
        The reference formulation (rml:referenceFormulation) defines the 
        reference formulation used to refer to the elements of the data source.
        The reference formulation must be specified in the case of databases 
        and XML and JSON data sources. By default SQL2008 for databases, as 
        SQL2008 is the default for R2RML, XPath for XML and JSONPath for JSON 
        data sources. 
        """ ;
            ns1:in ( <http://semweb.mmlab.be/ns/ql#CSV> <http://semweb.mmlab.be/ns/ql#TSV> ) ;
            ns1:maxCount 1 ;
            ns1:message """
        Reference formulation for CSV files must be ql:CSV.
        TSV files must use ql:TSV. Providing rml:referenceFormulation for 
        tabular data is optional.
        """ ;
            ns1:minCount 0 ;
            ns1:name "rml:referenceFormulation" ;
            ns1:path <http://semweb.mmlab.be/ns/rml#referenceFormulation> ] .

<http://schema.org/CSVWDialectShape> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:description """
    Dialect description for a CSVW Table. One csvw:delimiter, one csw:encoding,
    one csvw:doubleQuote, one csvw:quoteChar, one csvw:escapeChar, one 
    csvw:lineTerminators, one csvw:trim, one csvw:skipInitialSpace, one 
    csvw:header, one csvw:headerRowCount, one csvw:skipColumns, one 
    csvw:skipRows and one csvw:commentPrefix are optional.
    """ ;
    ns1:ignoredProperties ( rdf:type rdfs:label rdfs:comment ) ;
    ns1:message "CSVW Dialect violation" ;
    ns1:name "csvw:Dialect" ;
    ns1:property [ ns1:datatype xsd:string ;
            ns1:description """
        An atomic property that sets the line terminators flag to either an 
        array containing the single provided string value, or the provided 
        array.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        The line terminators must be a string with a length of at least 1.
        May only be provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:minLength 1 ;
            ns1:name "csvw:lineTerminators" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/csvw#lineTerminators> ],
        [ ns1:description """
        An atomic property that, if the boolean `true`, sets the trim flag to 
        `true` and if the boolean `false` to `false`. If the value provided is 
        a string, sets the trim flag to the provided value, which must be one 
        of "true", "false", "start" or "end".
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Trim mode must be either a boolean or a string with value "true", 
        "false", "start" or "end". May only be provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:name "csvw:trim" ;
            ns1:nodeKind ns1:Literal ;
            ns1:or ( [ ns1:datatype xsd:string ;
                        ns1:in ( "true" "false" "start" "end" ) ] [ ns1:datatype xsd:boolean ;
                        ns1:in ( true false ) ] ) ;
            ns1:path <http://www.w3.org/ns/csvw#trim> ],
        [ ns1:datatype xsd:boolean ;
            ns1:description """
        A boolean atomic property that, if `true`, sets the trim flag to 
        "start". If `false`, to `false`.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Skip initial space flag must be a boolean. May only be provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:name "csvw:skipInitialSpace" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/csvw#skipInitialSpace> ],
        [ ns1:datatype xsd:boolean ;
            ns1:description """
        A boolean atomic property that, if `true`, sets the header row count 
        flag to `1`, and if `false` to `0`, unless headerRowCount is provided, 
        in which case the value provided for the header property is ignored.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Header flag must be a boolean. May only be provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:name "csvw:header" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/csvw#header> ],
        [ ns1:datatype xsd:integer ;
            ns1:description """
        An numeric atomic property that sets the header row count flag to the 
        single provided value, which must be a non-negative integer.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        The header row count must be a non-negative integer. May only be 
        provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:minInclusive 0 ;
            ns1:name "csvw:headerRowCount" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/csvw#headerRowCount> ],
        [ ns1:datatype xsd:integer ;
            ns1:description """
        An numeric atomic property that sets the `skip columns` flag to the 
        single provided numeric value, which MUST be a non-negative integer.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Skip columns must be a non-negative integer. May only be provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:minInclusive 0 ;
            ns1:name "csvw:skipColumns" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/csvw#skipColumns> ],
        [ ns1:datatype xsd:integer ;
            ns1:description """
        An numeric atomic property that sets the `skip rows` flag to the single 
        provided numeric value, which MUST be a non-negative integer.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Skip rows must be a non-negative integer. May only be provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:minInclusive 0 ;
            ns1:name "csvw:skipRows" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/csvw#skipRows> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        An atomic property that sets the comment prefix flag to the single 
        provided value, which MUST be a string.
        """ ;
            ns1:maxCount 1 ;
            ns1:maxLength 1 ;
            ns1:message """
        Comment prefix must be a string and its length must be exactly 1.
        May only be provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:minLength 1 ;
            ns1:name "csvw:commentPrefix" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/csvw#commentPrefix> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        An atomic property that sets the delimiter flag to the single provided 
        value, which MUST be a string.
        """ ;
            ns1:maxCount 1 ;
            ns1:maxLength 1 ;
            ns1:message """
        The delimiter must be a string and its length must be exactly 1.
        May only be provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:minLength 1 ;
            ns1:name "csvw:delimiter" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/csvw#delimiter> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        An atomic property that sets the encoding flag to the single provided 
        string value, which MUST be a defined in [encoding]. The default is 
        "utf-8".

        [encoding] https://encoding.spec.whatwg.org/
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        The file encoding must match with the file encodings listed in 
        https://encoding.spec.whatwg.org/.
        May only be provided once.
        """ ;
            ns1:minCount 0 ;
            ns1:name "csvw:encoding" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/csvw#encoding> ;
            ns1:pattern "(unicode-1-1-utf-8)|(unicode11utf8)|(unicode20utf8)|(utf-8)|(utf8)|(x-unicode20utf8)|(866)|(cp866)|(csibm866)|(ibm866)|(csisolatin2)|(iso-8859-2)|(iso-ir-101)|(iso8859-2)|(iso88592)|(iso_8859-2)|(iso_8859-2:1987)|(l2)|(latin2)|(csisolatin3)|(iso-8859-3)|(iso-ir-109)|(iso8859-3)|(iso88593)|(iso_8859-3)|(iso_8859-3:1988)|(l3)|(latin3)|(csisolatin4)|(iso-8859-4)|(iso-ir-110)|(iso8859-4)|(iso88594)|(iso_8859-4)|(iso_8859-4:1988)|(l4)|(latin4)|(csisolatincyrillic)|(cyrillic)|(iso-8859-5)|(iso-ir-144)|(iso8859-5)|(iso88595)|(iso_8859-5)|(iso_8859-5:1988)|(arabic)|(asmo-708)|(csiso88596e)|(csiso88596i)|(csisolatinarabic)|(ecma-114)|(iso-8859-6)|(iso-8859-6-e)|(iso-8859-6-i)|(iso-ir-127)|(iso8859-6)|(iso88596)|(iso_8859-6)|(iso_8859-6:1987)|(csisolatingreek)|(ecma-118)|(elot_928)|(greek)|(greek8)|(iso-8859-7)|(iso-ir-126)|(iso8859-7)|(iso88597)|(iso_8859-7)|(iso_8859-7:1987)|(sun_eu_greek)|(csiso88598e)|(csisolatinhebrew)|(hebrew)|(iso-8859-8)|(iso-8859-8-e)|(iso-ir-138)|(iso8859-8)|(iso88598)|(iso_8859-8)|(iso_8859-8:1988)|(visual)|(csiso88598i)|(iso-8859-8-i)|(logical)|(csisolatin6)|(iso-8859-10)|(iso-ir-157)|(iso8859-10)|(iso885910)|(l6)|(latin6)|(iso-8859-13)|(iso8859-13)|(iso885913)|(iso-8859-14)|(iso8859-14)|(iso885914)|(csisolatin9)|(iso-8859-15)|(iso8859-15)|(iso885915)|(iso_8859-15)|(l9)|(iso-8859-16)|(cskoi8r)|(koi)|(koi8)|(koi8-r)|(koi8_r)|(koi8-ru)|(koi8-u)|(csmacintosh)|(mac)|(macintosh)|(x-mac-roman)|(dos-874)|(iso-8859-11)|(iso8859-11)|(iso885911)|(tis-620)|(windows-874)|(cp1250)|(windows-1250)|(x-cp1250)|(cp1251)|(windows-1251)|(x-cp1251)|(ansi_x3.4-1968)|(ascii)|(cp1252)|(cp819)|(csisolatin1)|(ibm819)|(iso-8859-1)|(iso-ir-100)|(iso8859-1)|(iso88591)|(iso_8859-1)|(iso_8859-1:1987)|(l1)|(latin1)|(us-ascii)|(windows-1252)|(x-cp1252)|(cp1253)|(windows-1253)|(x-cp1253)|(cp1254)|(csisolatin5)|(iso-8859-9)|(iso-ir-148)|(iso8859-9)|(iso88599)|(iso_8859-9)|(iso_8859-9:1989)|(l5)|(latin5)|(windows-1254)|(x-cp1254)|(cp1255)|(windows-1255)|(x-c