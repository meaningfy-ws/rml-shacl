            ns1:minCount 0 ;
            ns1:name "rml:query" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://semweb.mmlab.be/ns/rml#query> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
