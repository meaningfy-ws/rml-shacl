        [ ns1:class <http://www.w3.org/ns/dcat#Distribution> ;
            ns1:description """
        An available distribution of the dataset.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Exactly one dcat:distribution is required to access a distribution of 
        the data.
        """ ;
            ns1:minCount 1 ;
            ns1:name "dcat:distribution" ;
            ns1:node <http://schema.org/DCATDistributionShape> ;
            ns1:path <http://www.w3.org/ns/dcat#distribution> ] ;
