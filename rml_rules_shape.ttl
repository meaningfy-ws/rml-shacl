            ns1:path <http://www.w3.org/ns/sparql-service-description#resultFormat> ] ;
    ns1:targetClass <http://www.w3.org/ns/sparql-service-description#Service> .

<http://schema.org/SPARQLEndpointShape> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:ignoredProperties ( <http://semweb.mmlab.be/ns/rml#iterator> <http://semweb.mmlab.be/ns/rml#referenceFormulation> ) ;
    ns1:or ( [ ns1:and ( [ ns1:property [ ns1:in ( <http://semweb.mmlab.be/ns/ql#JSONPath> <http://semweb.mmlab.be/ns/ql#XPath> ) ;
                                    ns1:maxCount 1 ;
                                    ns1:minCount 1 ;
                                    ns1:path <http://semweb.mmlab.be/ns/rml#referenceFormulation> ] ] [ ns1:property [ ns1:datatype xsd:string ;
                                    ns1:maxCount 1 ;
                                    ns1:minCount 1 ;
                                    ns1:nodeKind ns1:Literal ;
                                    ns1:path <http://semweb.mmlab.be/ns/rml#iterator> ] ] ) ] [ ns1:and ( [ ns1:property [ ns1:in ( <http://semweb.mmlab.be/ns/ql#CSV> <http://semweb.mmlab.be/ns/ql#TSV> ) ;
                                    ns1:maxCount 1 ;
                                    ns1:minCount 0 ;
                                    ns1:path <http://semweb.mmlab.be/ns/rml#referenceFormulation> ] ] [ ns1:property [ ns1:datatype xsd:string ;
                                    ns1:maxCount 0 ;
                                    ns1:minCount 0 ;
                                    ns1:nodeKind ns1:Literal ;
                                    ns1:path <http://semweb.mmlab.be/ns/rml#iterator> ] ] ) ] ) ;
    ns1:property [ ns1:class <http://www.w3.org/ns/sparql-service-description#Service> ;
            ns1:description """
        The source (rml:source) locates the input data source. It is a URI
        that represents the data source where the data source is.
        
        Note: A literal as rml:source is still supported for compatibility with 
        previous versions of RML.
        """ ;
            ns1:message """
        rml:source must be provided to locate the data source. rml:source must 
        be provided exactly once as an URI or Literal for backwards 
        compatiblity with previous versions of RML.
        
        Access SPARQL endpoints requires a SD Service description.
        """ ;
            ns1:name "rml:source" ;
            ns1:node <http://schema.org/SDServiceShape> ;
            ns1:nodeKind ns1:BlankNodeOrIRI ;
            ns1:path <http://semweb.mmlab.be/ns/rml#source> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        The query to execute on the data source.
        rml:query is unofficially supported by RML, but not in the 
        specification.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        rml:query is required for querying a SPARQL endpoint.
        Must be provided once as a string.
        """ ;
            ns1:minCount 1 ;
            ns1:name "rml:query" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://semweb.mmlab.be/ns/rml#query> ] .

<http://schema.org/DCATDistributionShape> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:description """
    A DCAT Distribution is a specific representation of a dataset. A dataset 
    might be available in multiple serializations that may differ in various 
    ways, including natural language, media-type or format, schematic 
    organization, temporal and spatial resolution, level of detail or profiles 
    (which might specify any or all of the above). 
    """ ;
    ns1:ignoredProperties ( rdf:type rdfs:label rdfs:comment ),
        ( rdf:type rdfs:label rdfs:comment ) ;
    ns1:message """
    A DCAT Distribution requires exactly one dcat:downloadURL property.
    """ ;
    ns1:name "DCAT Dataset description: DCAT Distribution" ;
    ns1:property [ ns1:datatype xsd:string ;
            ns1:description """
        The URL of the downloadable file in a given format. E.g. CSV file or 
        RDF file. The format is indicated by the distribution's dct:format 
        and/or dcat:mediaType.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Exactly one dcat:downloadURL is required to download the dataset.
        """ ;
            ns1:minCount 1 ;
            ns1:name "dcat:downloadURL" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/dcat#downloadURL> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        The URL of the downloadable file in a given format. E.g. CSV file or 
        RDF file. The format is indicated by the distribution's dct:format 
        and/or dcat:mediaType.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Exactly one dcat:downloadURL is required to download the dataset.
        """ ;
            ns1:minCount 1 ;
            ns1:name "dcat:downloadURL" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/dcat#downloadURL> ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        The media type of the distribution as defined by IANA.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Exactly one dcat:mediaType may be provided. If none is provided, the 
        MIME type is guessed.
        """ ;
            ns1:name "dcat:mediaType" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/dcat#mediaType> ] ;
    ns1:targetClass <http://www.w3.org/ns/dcat#Distribution> .

<http://schema.org/RRGraphMapShape> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:description """
    Represents a graph map.
    """ ;
    ns1:ignoredProperties ( rdf:type rdfs:label rdfs:comment ) ;
    ns1:message """
    rr:GraphMap must specify an rr:template or rr:constant with the IRI of the 
    Named Graph.
    """ ;
    ns1:name "rr:graphMap" ;
    ns1:property [ ns1:description """
        Exactly one rr:template or one rr:constant is required.
        """ ;
            ns1:maxCount 1 ;
            ns1:message """
        Exactly one rr:template or one rr:constant is required.
        """ ;
            ns1:minCount 1 ;
            ns1:name "rr:template/rr:constant" ;
            ns1:path [ ns1:alternativePath ( <http://www.w3.org/ns/r2rml#template> <http://www.w3.org/ns/r2rml#constant> ) ] ],
        [ ns1:datatype xsd:string ;
            ns1:description """
        A template (format string) to specify how to generate a value for a 
        subject, predicate, or object, using one or more columns from a logical
        table row or iterator.
        """ ;
            ns1:message """
        rr:template must be a string.
        """ ;
            ns1:name "rr:template" ;
            ns1:nodeKind ns1:Literal ;
            ns1:path <http://www.w3.org/ns/r2rml#template> ],
        [ ns1:description """
        A property for indicating whether a term map is a constant-valued term 
        map.
        """ ;
            ns1:message """
        rr:constant must be an IRI.
        """ ;
            ns1:name "rr:constant" ;
            ns1:nodeKind ns1:IRI ;
            ns1:path <http://www.w3.org/ns/r2rml#constant> ] .

<http://schema.org/RRgraphShape> a ns1:NodeShape ;
    ns1:closed true ;
    ns1:description """
    An IRI reference for use as the graph name of all triples generated with 
    the GraphMap. 
    """ ;
    ns1:ignoredProperties ( rdf:type rdfs:label rdfs:comment ) ;
    ns1:message """
    rr:graph must be an IRI.
    """ ;
    ns1:name "rr:graph" ;
    ns1:nodeKind ns1:IRI .

<http://schema.org/DCATDatasetShape> a ns1:NodeShape ;
